{
  "pandora_core:ctm": "horizontal",
  "pandora_core:connect": "pandora_core:ctm_horizontal",
  "pandora_core:properties": [
    "axis"
  ],
  "pandora_core:overwrite": {
    "pandora_core:block/ctm_horizontal": "pandora_core:block/ctm_horizontal_sheet"
  },
  "variants": {
    "axis=x": {
      "model": "pandora_core:block/ctm_horizontal",
      "x": 90,
      "y": 90,
      "pandora_core:mutators":{
        "down": "rot_cw",
        "up": "rot_cw",
        "north": "rot_ccw",
        "south": "rot_cw"
      }
    },
    "axis=y": {
      "model": "pandora_core:block/ctm_horizontal"
    },
    "axis=z": {
      "model": "pandora_core:block/ctm_horizontal",
      "x": 90,
      "pandora_core:mutators":{
        "down": "inverted",
        "north": "inverted",
        "west": "rot_ccw",
        "east": "rot_cw"
      }
    }
  }
}